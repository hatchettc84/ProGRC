  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:local": "aws sso login && nest start --watch",
    "start:profile": "NODE_ENV=development IS_LOCAL=true aws sso login && nest start --watch",
    "start:profile:debug": "NODE_ENV=development IS_LOCAL=true nest start --debug --watch",
    "start:dev:simple": "NODE_ENV=development nest start --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:create": "npm run typeorm -- migration:create ./src/migrations/$npm_config_name",
    "migration:up": "npm run typeorm -- migration:run -d ./src/config/typeorm.config.ts",
    "migration:down": "npm run typeorm -- migration:revert -d ./src/config/typeorm.config.ts"
  },
