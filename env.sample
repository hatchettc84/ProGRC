# =======================
# SECRET PROVIDER CONFIGURATION
# =======================
# Choose your secret provider: aws, vault, gcp, azure
# If not set, no secrets will be loaded from external providers
SECRET_PROVIDER=aws

# Optional: Enable fallback to another provider if primary fails
SECRETS_ENABLE_FALLBACK=false
SECRETS_FALLBACK_BACKEND=

# Optional: Cache configuration
SECRETS_CACHE_ENABLED=true
SECRETS_CACHE_TTL=300

# Optional: Retry configuration
SECRETS_RETRY_ATTEMPTS=3
SECRETS_RETRY_DELAY=1000

# =======================
# AWS SECRETS MANAGER (when SECRET_PROVIDER=aws)
# =======================
# Option 1: Use IAM Role-based access (RECOMMENDED for production)
# Leave AWS_ACCESS_KEY_ID and AWS_SECRET_ACCESS_KEY empty to use:
# - EC2 Instance Profiles
# - ECS Task Roles  
# - Lambda Execution Roles
# - AWS SSO (for local development)
AWS_REGION=us-east-1

# =======================
# LOCAL/NON-AWS DEVELOPMENT
# =======================
# Set to true to point AWS SDK calls at LocalStack and avoid real AWS usage
USE_LOCALSTACK=true
LOCALSTACK_ENDPOINT=http://localhost:4566

# When using LocalStack, these defaults work without real AWS credentials
AWS_S3_ENDPOINT=http://localhost:4566
AWS_SQS_ENDPOINT=http://localhost:4566
# Disable SQS integration entirely by setting to false
AWS_SQS_ENABLED=true

# Option 2: Use explicit credentials (only for local development/testing)
# AWS_ACCESS_KEY_ID=your-access-key-id
# AWS_SECRET_ACCESS_KEY=your-secret-access-key

# =======================
# AZURE KEY VAULT (when SECRET_PROVIDER=azure)
# =======================
AZURE_KEY_VAULT_NAME=
AZURE_TENANT_ID=
AZURE_CLIENT_ID=
AZURE_CLIENT_SECRET=

# =======================
# GCP SECRET MANAGER (when SECRET_PROVIDER=gcp)
# =======================
GCP_PROJECT_ID=
GCP_CLIENT_EMAIL=
GCP_PRIVATE_KEY=

# =======================
# HASHICORP VAULT (when SECRET_PROVIDER=vault)
# =======================
VAULT_ADDR=http://localhost:8200
VAULT_TOKEN=
VAULT_AUTH_METHOD=token
VAULT_ROLE_ID=
VAULT_SECRET_ID=
VAULT_KUBERNETES_ROLE=
VAULT_KUBERNETES_JWT=

# =======================
# APPLICATION CONFIGURATION
# =======================
COGNITO_USER_POOL_ID=
COGNITO_CLIENT_ID=
COGNITO_REGION=us-east-1
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
OPENAI_API_KEY=
HUME_API_KEY=
HUME_API_URL=https://api.hume.ai

